{
  "name": "@apparts/config",
  "version": "1.5.0",
  "description": "A config-loading framework",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "git": "git@github.com:phuhl/apparts-config.git",
  "scripts": {
    "test": "BTEST=false TEST3='\"envtest\"' TEST4=ImVudnRlc3QyIg== REACT_APP_TEST5='\"react\"' VITE_TEST6='\"vite\"' vitest",
